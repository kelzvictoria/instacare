================


new home Component start
====================================

import React, { Component } from "react";

export interface homeProps {}

export interface homeState {}

class home extends React.Component<homeProps, homeState> {
  state = {};
  render() {
    return (
      <div className="home">
        <div className="col-md-12">
          <div className="col-md-5 home-white-card">
            <div className="heading">
              <h3>See plans & prices</h3>
            </div>
            <div className="fist-paragraph">
              <h5>Preview 2021 plans and estimated prices</h5>
              <p>
                You can browse plans here any time. Next, we'll send you to log
                in or create an account if you want to apply and enroll in a
                2021 plan.
              </p>
            </div>
            <div className="second-paragraph">
              <h5>You can still get 2021 health insurance</h5>
              <p>
                Enter your Zip Code & choose your location below to Compare and
                get HMO plans in Nigeria from the comfort of your home.
              </p>
            </div>
            <div className="third-paragraph">
              <div className="col-md-12">
                <label>Enter your ZIP Code & choose your location:</label>
                <div className="col-md-8">
                  <input
                    className="form-control"
                    placeholder="Example 100264"
                  />
                </div>
                <div className="col-md-4">
                  <button className="btn btn-primary btn-large" type="button">
                    Continue
                  </button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    );
  }
}

export default home;


new Component End
==================================

